<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="Sample">
			<!--<section name="Infrastructure" type="Infrastructure.Configuration.InfrastructureConfigSection, Infrastructure" />-->
			<section name="ClientApp" type="ClientApp.Configuration.ClientAppConfigSection, ClientApp" />
		</sectionGroup>
	</configSections>
	
	<startup> 
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>

	<Sample>
		<ClientApp>
			<ServiceFolders>
				<CardSourceGroup Key="1">
					<Members>
						<CardSourceNamedGroup DisplayNameResourceKey="ServiceFolders_CreatedForMe">
							<Members>
								<CardSource Name="Additional_TasksForMe_Undertaken" />
								<CardSource Name="Additional_TasksForMe_Overdued" />
								<CardSource Name="Additional_TasksForMe_Urgent" />
								<CardSource Name="Additional_TasksForMe_Completed" />
							</Members>
						</CardSourceNamedGroup>
					</Members>
				</CardSourceGroup>
				<CardSourceGroup Key="2">
					<Members>
						<CardSourceNamedGroup DisplayNameResourceKey="ServiceFolders_CreatedByMe">
							<Members>
								<CardSource Name="Additional_MyTasks_Initiated" />
								<CardSource Name="Additional_MyTasks_Drafts" />
								<CardSource Name="Additional_MyTasks_Overdued" />
								<CardSource Name="Additional_MyTasks_Expired" />
								<CardSource Name="Additional_MyTasks_Problems" />
								<CardSource Name="Additional_MyTasks_Completed" />
								<CardSource Name="Additional_MyTasks_Acceptance" />
							</Members>
						</CardSourceNamedGroup>
					</Members>
				</CardSourceGroup>
				<CardSourceGroup Key="3">
					<Members>
						<CardSource Name="Additional_Tasks_Tracking" />
					</Members>
				</CardSourceGroup>
				<CardSourceGroup Key="4">
					<Members>
						<CardSource Name="Additional_Tasks_Deleted" />
					</Members>
				</CardSourceGroup>
			</ServiceFolders>

			<Semaphors>
				<CardSourceNamedGroup DisplayNameResourceKey="SemaphorGroup_ForMe">
					<Members>
						<CardSource Name="New" />
						<CardSource Name="ToDo" />
					</Members>
				</CardSourceNamedGroup>
				<CardSourceNamedGroup DisplayNameResourceKey="SemaphorGroup_Returned">
					<Members>
						<CardSource Name="Problems" />
						<CardSource Name="OnAcceptance" />
					</Members>
				</CardSourceNamedGroup>
				<CardSourceNamedGroup DisplayNameResourceKey="SemaphorGroup_FromMe">
					<Members>
						<CardSource Name="Complete" />
						<CardSource Name="Expiring" />
					</Members>
				</CardSourceNamedGroup>
			</Semaphors>
		</ClientApp>
	</Sample>

</configuration>